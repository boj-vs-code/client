{
  "name": "boj-vs-code",
  "displayName": "boj-vs-code",
  "description": "",
  "version": "0.1.0",
  "publisher": "bojvscode",
  "repository": "https://github.com/moreal/boj-vs-code",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:/.bojconfig"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.submitBOJ",
        "key": "ctrl+alt+s"
      },
      {
        "command": "extension.showProblemInformation",
        "key": "ctrl+alt+i"
      }
    ],
    "commands": [
      {
        "command": "extension.submitBOJ",
        "title": "Run BOJ Testcases"
      },
      {
        "command": "extension.showProblemInformation",
        "title": "whwww"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && cd out && mocha --ui tdd"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "axios": "^0.18.0",
    "dom-parser": "^0.1.5",
    "node-html-parser": "^1.1.12",
    "tslint": "^5.8.0",
    "typescript": "^3.1.4",
    "vscode": "^1.1.25"
  },
  "dependencies": {
    "@types/pusher-js": "^4.2.0",
    "pusher-js": "^4.4.0"
  }
}
